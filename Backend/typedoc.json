{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/**/*.ts"],
  "out": "docs/api",
  "name": "QuizUP Backend API",
  "includeVersion": true,
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.ts"
  ],
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "disableSources": false,
  "readme": "README.md",
  "tsconfig": "tsconfig.json",
  "theme": "default",
  "cleanOutputDir": true,
  "blockTags": ["@example", "@deprecated", "@see", "@link"],
  "inlineTags": ["@link", "@linkcode", "@linkplain"],
  "modifierTags": ["@public", "@private", "@protected", "@internal"],
  "gitRevision": "main",
  "gitRemote": "origin",
  "logLevel": "Info",
  "validation": {
    "invalidLink": true,
    "notDocumented": false,
    "notExported": false
  }
}
