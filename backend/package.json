{
  "name": "quiz-app-backend",
  "version": "1.0.0",
  "description": "Backend for real-time quiz application",
  "main": "dist/server.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:match": "node dist/matchServer.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:match\"",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:match": "ts-node-dev --respawn --transpile-only src/matchServer.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:match\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node dist/prisma/seed.js",
    "seed:massive": "ts-node src/scripts/seedDatabase.ts",
    "seed:quick": "ts-node --transpile-only src/scripts/quickSeed.ts",
    "seed:dev": "ts-node-dev --respawn --transpile-only src/scripts/seedDatabase.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern='(categories|quizzes|questions|auth|validation|health|database)\\.test\\.ts'",
    "docker:build": "docker build -t quiz-app-backend .",
    "docker:up": "docker-compose up --build"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-prom-bundle": "^7.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.13",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/winston": "^2.4.4",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "quiz",
    "backend",
    "express",
    "prisma",
    "redis",
    "typescript"
  ],
  "author": "Quiz App Team",
  "license": "MIT"
}
