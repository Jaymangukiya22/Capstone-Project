{
  "name": "quiz-app-backend",
  "version": "1.0.0",
  "description": "Backend for real-time quiz application",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "node pre-build.js",
    "build": "tsc",
    "postbuild": "echo 'âœ… TypeScript compilation completed'",
    "start": "node dist/server.js",
    "start:match": "node dist/matchServer-enhanced.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:match\"",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:match": "ts-node-dev --respawn --transpile-only src/matchServer-enhanced.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:match\"",
    "db:setup": "node dist/scripts/setupDatabase.js",
    "seed:massive": "ts-node src/scripts/seedDatabase.ts",
    "seed:quick": "ts-node --transpile-only src/scripts/quickSeed.ts",
    "seed:dev": "ts-node-dev --respawn --transpile-only src/scripts/seedDatabase.ts",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
    "test:coverage-html": "jest --coverage --coverageReporters=html --detectOpenHandles --forceExit",
    "test:integration": "jest --testPathPattern=integration --detectOpenHandles --forceExit",
    "test:unit": "jest --testPathPattern=unit --detectOpenHandles --forceExit",
    "test:verbose": "jest --verbose --detectOpenHandles --forceExit",
    "test:runner": "ts-node tests/testRunner.ts",
    "docker:build": "docker build -t quiz-app-backend .",
    "docker:up": "docker compose up --build",
    "docker:verify": "node pre-build.js",
    "verify": "node pre-build.js",
    "docs:typedoc": "typedoc --options typedoc.json",
    "docs:generate": "npm run docs:typedoc"
  },
  "dependencies": {
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-prom-bundle": "^7.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/theme-classic": "^3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@faker-js/faker": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.17",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "@types/winston": "^2.4.4",
    "@types/yaml": "^1.9.6",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "quiz",
    "backend",
    "express",
    "sequelize",
    "redis",
    "typescript"
  ],
  "author": "Quiz App Team",
  "license": "MIT"
}
